<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#FAFAFA"
  >
    <RelativeLayout
        android:background="#fff"
        android:padding="10dp"
        android:layout_marginBottom="20dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/setting_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/top_back"/>
        <TextView
        android:layout_centerHorizontal="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设置"/>
    </RelativeLayout>
    <RelativeLayout
       android:onClick="settingclick"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:id="@+id/setting_unsaving"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:layout_centerVertical="true"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="清除缓存"
            />
        <TextView
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:id="@+id/getting_saving"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="4.3M"
            />

    </RelativeLayout>
    <RelativeLayout
        android:onClick="settingclick"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:id="@+id/setting_suggestion"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:layout_centerVertical="true"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="意见反馈"
            />
        <ImageView
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:id="@+id/getting_forward"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/found_item_right_arrow"
            />
        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#0ff"/>

    </RelativeLayout>
    <RelativeLayout
        android:onClick="settingclick"
        android:gravity="center"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:id="@+id/setting_updateversion"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:layout_centerVertical="true"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="版本更新"
            />

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#0ff"/>

    </RelativeLayout>
    <RelativeLayout
        android:onClick="settingclick"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:id="@+id/setting_about"
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:layout_centerVertical="true"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="关于"
            />
        <ImageView
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/found_item_right_arrow"
            />
        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="#0ff"/>

    </RelativeLayout>


</LinearLayout>
